    "fig, axs = plt.subplots(1, 3, sharey=True)\n",
    "\n",
    "params = [\"TV\", \"radio\", \"newspaper\"]\n",
    "\n",
    "sales = data['sales']\n",
    "\n",
    "for i, param in enumerate(params):\n",
    "    pred = np.array(data[param])\n",
    "    pred = pred[:, np.newaxis]\n",
    "    reg = LinearRegression().fit(pred, sales)\n",
    "    x = np.linspace(0, pred.max(), 10)\n",
    "    y = reg.intercept_ + reg.coef_ * x\n",
    "    axs[i].scatter(x=pred, y=sales, marker='o', facecolor='none', edgecolors='red')\n",
    "    axs[i].plot(x, y)\n",
    "\n",
    "axs[0].set_ylabel('Sales')\n",
    "axs[0].set_xlabel('TV')\n",
    "axs[1].set_xlabel('Radio')\n",
    "axs[2].set_xlabel('Newspaper')"
